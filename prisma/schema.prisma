generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model examdata {
  id Int @id @default(autoincrement())
  exam_year Int
  grade Int
  subject String
  problem_statement String @db.Text
  problem_img String @db.Text
  choices Json
  choices_img_path Json
  correct Int
  explanation String @db.Text
}